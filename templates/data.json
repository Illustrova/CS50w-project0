    "forms": {
        "fields": [
            {
                "id": "pet",
                "type": "input",
                "label": "Pet kind",
                "sublabel": "What kind of pet is lost?",
                "placeholder": "e.g. dog, cat, fish...?",
                "required": true
            },
            {
                "id": "nickname",
                "type": "input",
                "label": "Nickname",
                "sublabel": "Fill it if there is a chance a pet will respond to its nickname",
                "placeholder": "e.g. Jackie",
                "hide": "found"
            },
            {
                "id": "breed",
                "type": "input",
                "label": "Breed",
                "sublabel": "If applicable",
                "placeholder": "e.g. German shepherd"
            },
            {
                "id": "age",
                "type": "input",
                "data": "number",
                "label": "Age",
                "sublabel": "More or less",
                "placeholder": "7",
                "hide": "found"
            },
            {
                "id": "color",
                "type": "input",
                "label": "Color",
                "sublabel": "Only main colors. For more detailed description use the field below",
                "placeholder": "Black and white"
            },
            {
                "id": "description",
                "type": "textarea",
                "label": "Pet description",
                "sublabel": "Describe in details how your pet looks. Does it have special signs or wears a collar?"
            },
            {
                "id": "behaviour",
                "type": "textarea",
                "label": "Pet behaviour",
                "sublabel": "Describe your pet's typical behaviour. Can it be dangerous? Or it afraids of people? Should one try to catch it?",
                "hide": "found"
            },
            {
                "id": "seen_date",
                "type": "input",
                "data": "date",
                "label": "Date",
                "required": true
            },
            {
                "id": "seen_location",
                "type": "input",
                "label": "Location",
                "sublabel": "Specify area at least, ideally type the exact address. This is very important information!",
                "required": true
            },
            {
                "id": "seen_details",
                "type": "textarea",
                "label": "Details",
                "sublabel": "Provide more details if needed"
            },
            {
                "id": "contacts_phone",
                "type": "input",
                "data": "phone",
                "label": "Phone"
            },
            {
                "id": "contacts_landline",
                "type": "input",
                "data": "phone",
                "label": "Landline",
                "sublabel": "or any additional phone"
            },
            {
                "id": "contacts_email",
                "type": "input",
                "data":"email",
                "label": "Email",
                "required": true
            },
            {
                "id": "reward",
                "type": "input",
                "data": "checkbox",
                "label": "Do you offer reward? ",
                "hide": "found"
            },
            {
                "id": "reward-amount",
                "type": "input",
                "data": "number",
                "placeholder": 100,
                "label": "Reward amount (Euro)",
                "sublabel": "This is not necessary to include, but this information can attract more attention to your alert",
                "hide": "found"
            }
        ]
    },

